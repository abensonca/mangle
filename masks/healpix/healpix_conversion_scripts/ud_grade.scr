#!/bin/tcsh
# USAGE:   ud_grade.scr <outres> <infile> <outfile>
# EXAMPLE: ud_grade.scr 128 qaz1.fits qaz2.fits
set res     = $1
set infile  = $2
set outfile = $3
echo ud_grade.scr $res $infile $outfile
echo 'nside_out = '$res   >qaz_ud_grade.dat
echo 'infile = '$infile >>qaz_ud_grade.dat
echo 'outfile = '$outfile >>qaz_ud_grade.dat
if -f $outfile /bin/rm $outfile
ud_grade qaz_ud_grade.dat
